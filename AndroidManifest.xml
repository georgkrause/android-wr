<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sebastiangeorg.wr.rechner"
    android:installLocation="auto"
    android:versionCode="0010102"
    android:versionName="0.1.1" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:smallScreens="true" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Rechner"
            android:label="@string/title_activity_rechner" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.georgkrause.androidwr.depreciation.ShowCalculator"
            android:label="@string/title_activity_ergebnis" >
        </activity>
    </application>

</manifest>